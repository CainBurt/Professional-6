{% block html_head_container %}
	{% include 'components/html-header.twig' %}
	{% block head %}
	{% endblock %}
	

	<script type='text/javascript' src="{{ theme.link ~ '/src/js/jquery-1.12.4.min.js' }}"></script>

	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		{{ options.body_start_scripts }}

		{% set main_logo = Image(options.main_logo) %}
		{% include "/components/header.twig" %}


		<main>
						
			<div  data-scroll-container  class="{{ post.slug }}">
				{% block content %}
					Sorry, no content
				{% endblock %}

				{% block footer %}
					{{ function('wp_footer') }}
				{% endblock %}
			</div>	
		</main>

        
        {{ function('stop_timber_timer') }} 
		
		{{options.body_end_scripts}}
		
		
		<script type='text/javascript'>
			var crowdAjax = "{{fn('admin_url', 'admin-ajax.php')}}";
		</script>
		

		<script type='text/javascript' src="{{ theme.link ~ '/dist/js/bundle.js' }}"></script>
		<script type='text/javascript' src="{{ theme.link ~ '/src/js/index.js' }}"></script>
	
	</body>
</html>
